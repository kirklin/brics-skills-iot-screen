{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "types"],
  "compilerOptions": {
    "baseUrl": "./",
    "types": ["vite/client", "element-plus/global"],
    "paths": {
      "@/*": ["src/*"],
      "api/*": [
        "src/api/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
